{
  "name": "sovran-group-website",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@fortawesome/fontawesome-free": "^7.0.0",
    "@headlessui/react": "^2.2.6",
    "@heroicons/react": "^2.2.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/react-helmet": "^6.1.11",
    "@types/react-router-dom": "^5.3.3",
    "firebase": "^12.1.0",
    "framer-motion": "^12.23.9",
    "gsap": "^3.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-router-dom": "^6.22.3",
    "react-scripts": "5.0.1",
    "react-social-media-embed": "^2.5.18",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "build:optimized": "npm install glob sharp --save-dev && node scripts/analyze_image_usage.js && react-scripts build && node scripts/copy_used_images.js && node scripts/resize_large_images.js && node scripts/resize_large_videos.js",
    "build:full-optimize": "npm install glob sharp --save-dev && react-scripts build && ./scripts/optimize_build.sh",
    "resize-images": "node scripts/resize_large_images.js",
    "resize-videos": "node scripts/resize_large_videos.js",
    "resize-media": "node scripts/resize_large_images.js && node scripts/resize_large_videos.js",
    "optimize-images": "node scripts/analyze_image_usage.js && node scripts/copy_used_images.js",
    "optimize-build": "./scripts/optimize_build.sh",
    "build:no-media": "react-scripts build && find build -type f \\( -iname '*.png' -o -iname '*.jpg' -o -iname '*.jpeg' -o -iname '*.gif' -o -iname '*.webp' -o -iname '*.svg' -o -iname '*.mp4' -o -iname '*.mov' -o -iname '*.webm' \\) -delete",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "autoprefixer": "^10.4.21",
    "glob": "^11.0.3",
    "postcss": "^8.5.6",
    "sharp": "^0.34.4",
    "tailwindcss": "^3.4.17"
  }
}
